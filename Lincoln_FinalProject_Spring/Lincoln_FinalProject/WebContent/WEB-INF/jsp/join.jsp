  <div class="register-box-body">
    <p class="login-box-msg">Register a new membership</p>

    <form action="#####" method="post">
      <div class="form-group has-feedback col-xs-9">
        <input type="text" class="form-control" placeholder="Firefighter-Code"> 
        <span class="glyphicon glyphicon-user form-control-feedback" style="margin-right:10px;"></span>
      </div>
      <div>
      	<button class="btn btn-primary btn-block btn-danger" style="width:65px;">Check</button>
      </div>
      <div class="form-group has-feedback col-xs-9">
        <input type="email" class="form-control" placeholder="IP">
        <span class="glyphicon glyphicon-info-sign form-control-feedback " style="margin-right:10px;"></span>
      </div>
      <div>
      	<button class="btn btn-primary btn-block btn-danger" style="width:65px; margin-top:15px;">Check</button>
      </div>
      <div class="form-group has-feedback col-xs-12">
        <input type="email" class="form-control" placeholder="Name">
